{
    "required_api_version": "2",
    "name": "RandomwallU",
    "description": "A ulauncher extensions, which lets you easily set random wallpapers.",
    "developer_name": "Fabian",
    "icon": "images/icon.jpeg",
    "options": {
      "query_debounce": 0.02
    },
    "preferences": [
      {
        "id": "wallpaper_kw",
        "type": "keyword",
        "name": "Wallpaper",
        "description": "",
        "default_value": "wp"
      },
      {
        "id": "api_key",
        "type": "text",
        "name": "API Key form Unsplash",
        "description": "Get a Access Key from https://unsplash.com/developers",
        "default_value": ""
      },
      {
        "id": "service",
        "type": "select",
        "name": "Service for Wallpapers",
        "description": "",
        "options": ["Unsplash", "Wallhaven.cc"],
        "default_value": "Wallhaven.cc"
      },
      {
        "id": "copy_location",
        "type": "text",
        "name": "Copy location",
        "description": "If you wish to copy the current wallpaper to a custom location, e.g. your wallpaper folder, input the location here.",
        "default_value": ""
      },
      {
        "id": "copy_shortcut",
        "type": "text",
        "name": "Copy Shortcut",
        "description": "The shortcut to copy.",
        "default_value": "cp"
      }
    ]
  }
  
